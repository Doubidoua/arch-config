{
   "author": "Andy Portmen",
   "background": {
      "scripts": [ "common.js" ]
   },
   "description": "Adds a context menu item to send audio/video streams directly to VLC media player",
   "homepage_url": "http://add0n.com/open-in.html?from=vlc",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Open in VLC media player",
   "options_ui": {
      "chrome_style": true,
      "page": "data/options/index.html"
   },
   "page_action": {
      "default_icon": {
         "16": "data/icons/16.png",
         "32": "data/icons/32.png",
         "64": "data/icons/64.png"
      }
   },
   "permissions": [ "storage", "tabs", "contextMenus", "nativeMessaging", "downloads", "webRequest", "\u003Call_urls>" ],
   "short_name": "ioivmplayer",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.5",
   "web_accessible_resources": [ "/data/inject/*" ]
}
